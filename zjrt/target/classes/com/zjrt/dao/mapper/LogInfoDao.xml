<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.zjrt.dao.LogInfoDao">
    <insert id="addLog">
        INSERT INTO logInfo(userid,username,logtime,type)
        VALUES (#{userid},#{username},#{logtime},#{type})
    </insert>
</mapper>